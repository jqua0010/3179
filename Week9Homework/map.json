{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 600,
    "height": 400,
    "background": "#e0f7fa",
    "layer": [
      {
        "data": {
          "graticule": true
        },
        "mark": {
          "type": "geoshape",
          "stroke": "#cccccc",
          "strokeWidth": 0.5
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/jqua0010/3179/main/Week9Homework/map_data.topojson",
          "format": {
            "type": "topojson",
            "feature": "STE_2016_AUST"
          }
        },
        "transform": [
          {
            "lookup": "properties.STE_NAME16",
            "from": {
              "data": {
                "url": "https://raw.githubusercontent.com/jqua0010/3179/main/Week9Homework/aggregated_livestock_2022.csv",
                "format": {"type": "csv"}
              },
              "key": "State",
              "fields": ["Value"]  
            }
          }
        ],
        "mark": {
          "type": "geoshape",
          "stroke": "#000000",
          "strokeWidth": 0.5
        },
        "encoding": {
          "color": {
            "field": "Value",
            "type": "quantitative",
            "scale": {
              "domain": [0, 100],  
              "range": ["#f7fbff", "#08306b"]  
            }
          },
          "tooltip": [
            {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
            {"field": "Value", "type": "quantitative", "title": "Value of Country (%)"}
          ]
        }
      }
    ],
    "projection": {
      "type": "mercator",
      "scale": 400,
      "translate": [-620, 0]
    }
  }
  