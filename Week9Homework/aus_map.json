{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "projection": {
      "type": "equalEarth",
      "scale": 600,
      "translate": [400, 300]
    },
    "data": {
      "url": "https://raw.githubusercontent.com/jqua0010/3179/main/Week9Homework/map_data.topojson",
      "format": {"type": "topojson", "feature": "STE_2016_AUST"}
    },
    "transform": [
      {
        "lookup": "properties.STE_NAME16",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/jqua0010/3179/main/Week9Homework/aggregated_livestock_2022.csv",
            "format": {"type": "csv"}
          },
          "key": "State",
          "fields": ["Value"]
        },
        "default": 0 
      }
    ],
    "mark": {
      "type": "geoshape",
      "fill": "#cccccc",
      "stroke": "#000000",
      "strokeWidth": 0.5
    },
    "encoding": {
      "color": {
        "field": "Value",
        "type": "quantitative",
        "scale": {"scheme": "blues"}
      },
      "tooltip": [
        {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
        {"field": "Value", "type": "quantitative", "title": "Total Livestock"}
      ]
    }
  }
  