{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "projection": {
      "type": "equalEarth",
      "scale": 600,
      "translate": [400, 300]
    },
    "data": {
      "url": "victoria_only_map_data.topojson",  
      "format": {
        "type": "topojson",
        "feature": "STE_2016_AUST"
      }
    },
    "mark": {
      "type": "geoshape",
      "fill": "#cccccc",
      "stroke": "#000000",
      "strokeWidth": 0.5
    },
    "encoding": {
      "tooltip": [
        {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"}
      ]
    }
  }
  